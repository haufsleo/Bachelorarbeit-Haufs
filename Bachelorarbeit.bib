institution = {Gesellschaft für Informatik},
keywords = {DIN EN ISO 9241-11,Logistik,Smart Glasses,Usability,Wearables},
mendeley-tags = {DIN EN ISO 9241-11,Logistik,Smart Glasses,Usability,Wearables},
title = {{Augmented Reality am Arbeitsplatz der Zukunft: Ein Usability-Framework für Smart Glasses}},
