journal = {International Conference on Wirtschaftsinformatik (WI2017)},
keywords = {13 th international conference,2017,dienstleistungssysteme,digitalisierung von logistikprozessen,fallstudie,february 12-15,gallen,on wirtschaftsinformatik,smart glasses,st,switzerland},
